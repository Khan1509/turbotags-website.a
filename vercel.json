{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node@2.0.0",
      "config": {
        "runtime": "nodejs22.x",
        "includeFiles": ["api/**"]
      }
    },
    {
      "src": "*.{html,txt,xml,json,ico}",
      "use": "@vercel/static"
    },
    {
      "src": "icons/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
