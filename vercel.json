{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/blog/$1"
    },
    {
      "src": "/legal/(.*)",
      "dest": "/legal/$1"
    },
    {
      "src": "/generator/(.*)",
      "dest": "/generator/$1"
    },
    {
      "src": "/(favicon\\.svg|manifest\\.json|styles\\.css|robots\\.txt|sitemap\\.xml|.*\\.html|.*\\.txt|screenshots/.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
